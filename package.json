{
  "name": "webcell-scaffold",
  "version": "0.1.0",
  "keywords": [
    "web-component",
    "typescript",
    "jsx"
  ],
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/scaffold.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/scaffold/issues"
  },
  "main": "src/index.html",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "2.4.0",
    "boot-cell": "^0.18.1",
    "cell-router": "^2.0.0-rc.3",
    "classnames": "^2.2.6",
    "mobx": "^5.15.1",
    "mobx-web-cell": "^0.2.5",
    "web-cell": "2.0.0-rc.12"
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{html,md,css,less,js,json,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "test": "lint-staged",
    "start": "parcel src/index.html --open",
    "build": "parcel build src/index.html --public-url ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm run build"
    }
  }
}
